{
  "name": "Medium-Freedium",
  "version": "1.0.0",
  "description": "Opens Medium article links on freedium.cfd website",
  "manifest_version": 3,
  "author": "Adarsh Shete",

  "content_scripts": [
    {
      "js": ["content.js"],
      "css": ["style.css"],
      "matches": ["https://medium.com/*", "https://*.medium.com/*"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "19": "images/icon19.png",
    "32": "images/icon32.png",
    "38": "images/icon38.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
